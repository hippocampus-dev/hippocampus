{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "window:allow-set-position",
    "window:allow-set-size",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "global-shortcut:default",
    "autostart:default",
    "updater:default",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save"
  ]
}
